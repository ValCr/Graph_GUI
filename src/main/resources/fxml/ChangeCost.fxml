<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxWidth="1.7976931348623157E308" prefHeight="127.0" prefWidth="354.0"
      spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="controllers.ChangeCostController">
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <TextField fx:id="text" maxWidth="1.7976931348623157E308" prefHeight="26.0" prefWidth="297.0"
               promptText="Enter cost"/>
    <TextField fx:id="capacity" maxWidth="1.7976931348623157E308" prefHeight="26.0" prefWidth="239.0"
               promptText="Enter capacity"/>
    <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
        <Label fx:id="warnings" maxWidth="1.7976931348623157E308" prefHeight="35.0" prefWidth="233.0"
               wrapText="true" HBox.hgrow="ALWAYS"/>
        <Button mnemonicParsing="false" onAction="#submit" text="Submit"/>
    </HBox>
</VBox>
